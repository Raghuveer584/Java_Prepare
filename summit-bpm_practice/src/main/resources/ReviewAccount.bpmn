      <bpmn:outgoing>SequenceFlow_1nkg1iy</bpmn:outgoing>
      <bpmn:outgoing>SequenceFlow_1im8ety</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="SequenceFlow_1nkg1iy" name="Yes" sourceRef="ExclusiveGateway_16cxk44" targetRef="EndEvent_03zojpt">
      <bpmn:documentation>Active</bpmn:documentation>
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${WsResponse== 200}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="UserTask_060341t" name="Pending for Reviewing the issue" camunda:candidateGroups="rhCSR_Supervisor,rhAdministrator">
      <bpmn:documentation>Check why Accoun Creation Failed Or need to send again</bpmn:documentation>
      <bpmn:incoming>SequenceFlow_1im8ety</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_13vi987</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="SequenceFlow_1im8ety" name="No" sourceRef="ExclusiveGateway_16cxk44" targetRef="UserTask_060341t">
      <bpmn:documentation>Check if Account Create successful or not</bpmn:documentation>
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${WsResponse!= 200}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:exclusiveGateway id="ExclusiveGateway_090jucm" name="Corected">
